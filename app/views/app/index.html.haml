!!! 5
%html
  %head
    %meta{charset: 'utf-8'}
    %title gjcase app
    = stylesheet_link_tag 'application' , media: 'all'
    = javascript_include_tag 'application'
    = csrf_meta_tags
  %body{data: {gj_url: Rails.application.secrets.gj_url}}
    -#.hidden
      %datalist#tags_datalist
        - Tag.active.each do |tag|
          %option{value: tag.name, "data-tag_id": tag.id.to_s}= tag.name
    = react_component('AppController')
